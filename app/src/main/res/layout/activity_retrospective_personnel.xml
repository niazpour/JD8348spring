<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.roshnighosh.costoutbreaktool.RetrospectivePersonnel">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="294dp"
        android:layout_height="466dp"
        android:layout_marginEnd="45dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="7dp"
        android:text="	Please report the total number of hours that you worked before receiving this tool (if any) in nearest quarter hour increments (e.g. when working from 4:00 to 5:10 round up for reporting purposes to 1.25 hours).
	Outbreak investigation hours include all efforts both directly or indirectly related to the investigation (e.g., case interviews, travel time, update meetings with leadership, time spent packaging samples to be sent to laboratories, administrative time setting up meetings and coordinating efforts, etc.).

"
        android:textAppearance="@style/TextAppearance.AppCompat.Caption"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Retrospective Personnel"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="7dp" />
</android.support.constraint.ConstraintLayout>
